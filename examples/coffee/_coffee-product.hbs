{
    "__typename": "Product",
    "id": "gid://shopify/Product/{{faker 'number.int' min=1000000000 max=9999999999}}",
    "availableForSale": true,
    "title": "{{coffee-name}}",
    "productType": "Coffee",
    "createdAt": "{{faker 'date.recent' }}",
    "publishedAt": "{{faker 'date.recent' }}",
    "updatedAt": "{{faker 'date.recent' }}",
    "handle": "{{faker 'lorem.slug' 5}}",
    "vendor": "{{faker 'company.name' }}",
    "description": "Our classic Breakfast Blend has a smooth, mild taste with low acidity and a light body. Itâ€™s our recommended morning cup of coffee",
    "seo": {
        "description": "The best coffee in the world",

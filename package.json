{
  "name": "@raysca/fake-server",
  "description": "A fake server for REST and GraphQL",
  "module": "out/main.js",
  "type": "commonjs",
  "main": "lib/main.js",
  "bin": {
    "fake-server": "lib/main.js"
  },
  "scripts": {
    "rest": "ts-node ./src/index.ts rest",
    "dev:graphql:coffee": "ts-node ./src/main.ts graphql -s examples/coffee/schema.graphql -d examples/coffee",
    "watch:graphql": "nodemon",
    "build": "tsc",
    "lint": "eslint src/**/*.ts",
    "start": "node lib/main.js",
    "test": "jest src/**/*.test.ts"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/yargs": "^17.0.28",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "bun-types": "latest",
    "eslint": "^8.53.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@faker-js/faker": "^8.1.0",
    "express": "^4.18.2",
    "graphql": "^16.8.1",
    "graphql-yoga": "^5.0.0",
    "handlebars": "^4.7.8",
    "object-path": "^0.11.8",
    "yargs": "^17.7.2"
  }
}
